{
  "parser": "babel-eslint",
  "parserOptions": {
        "ecmaVersion": 6,
         "sourceType": "module"
    },
  "extends": [
    "google",
    "plugin:react/recommended"
    ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "plugins": [
    "babel",
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "React$Element": true,
    "__DEV__": true
  },
  "rules": {
    "valid-jsdoc": ["warn"],
    "no-use-before-define": "off"
  }
}
